/* pages/i-pages/additional/additional.wxss */


.mine_item{
  overflow: hidden;
}
.mine_cell{
  background: #F4F4F4;
  overflow: hidden;
}
.mine_row{
  overflow: hidden;
  display:  -webkit-flex;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-content: flex-start;
  justify-content: flex-start;
}
.mine_row .mine_col:first-child{
  margin-left:-1px;
  margin-right:1px;
}
.mine_col{
  height:200rpx;
  width:50%;
  background-color:#FFFfFF; 
  text-align: center;
  flex: 1;
  box-sizing: border-box;
  margin-bottom: 1px;
}
.mine_col .mine_icon{
  width:76rpx;
  height:76rpx;
  display: block;
  margin:36rpx auto 16rpx;
}
.mine_col text{
  font-size:30rpx;
  text-align: center;
  color:#666666;
}
.mine_col .icon2{
  width:58rpx;
  height:80rpx;
}