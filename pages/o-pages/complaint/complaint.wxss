/* pages/o-pages/complaint/complaint.wxss */

.s_cell{
  padding:0 20rpx;
}
.s_cell .s_item{
  display: -webkit-flex;
  display: flex;
  height:88rpx;
  width:100%;
  border-bottom:1px solid #e9e9e9;
  justify-content:center;
  align-items:center;
} 
.s_cell .s_item:last-child{
  border:none;
}

.s_item .s_title{
  width:180rpx;
}
.s_item .s_value{
  flex: 3;
}

.com_cell{
  padding: 20rpx;
}
.com_title{
  height:40rpx;
  line-height: 40rpx;
}
.com_content{
  height:250rpx;
  margin-top:20rpx;
}


